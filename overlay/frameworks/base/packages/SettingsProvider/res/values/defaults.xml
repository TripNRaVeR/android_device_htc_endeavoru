<resources>
    <!-- 1 minutes for default screen timeout -->
    <integer name="def_screen_off_timeout">60000</integer>
    <!-- Set the automatic brightness mode on by default -->
    <bool name="def_screen_brightness_automatic_mode">true</bool>
    <!-- Non market apps -->
    <bool name="def_install_non_market_apps">true</bool>
    <!-- Device provisioned -->
    <bool name="def_device_provisioned">true</bool>
    <!-- Disable the package verifier-->
    <bool name="def_package_verifier_enable">false</bool>
</resources>
